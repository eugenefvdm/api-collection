{
    "name": "eugenefvdm/api-collecton",
    "description": "APIs for various services",
    "license": "MIT",
    "keywords": [
        "api",
        "apis",
        "laravel",
        "php",
        "bulksms",
        "discord",
        "hellopeter",
        "slack",
        "telegram",
        "zadomains"
    ],
    "authors": [
        {
            "name": "Eugene van der Merwe",
            "email": "hello@eugenefvdm.com",
            "homepage": "https://eugenefvdm.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/eugenefvdm/api",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.12|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Eugenefvdm\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eugenefvdm\\Api\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eugenefvdm\\Api\\ApiServiceProvider"
            ]
        }
    }
}
